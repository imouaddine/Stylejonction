<h1>Listing videos</h1>

<table>
  <tr>
    <th>Caption</th>
    <th>Url</th>
    <th>Gallery</th>
    <th>Weight</th>
    <th>Provider name</th>
    <th>Thumbnail width</th>
    <th>Thumbnail height</th>
    <th>Thumbnail url</th>
    <th>Html</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @videos.each do |video| %>
  <tr>
    <td><%= video.caption %></td>
    <td><%= video.url %></td>
    <td><%= video.gallery_id %></td>
    <td><%= video.weight %></td>
    <td><%= video.provider_name %></td>
    <td><%= video.thumbnail_width %></td>
    <td><%= video.thumbnail_height %></td>
    <td><%= video.thumbnail_url %></td>
    <td><%= video.html %></td>
    <td><%= link_to 'Show', video %></td>
    <td><%= link_to 'Edit', edit_video_path(video) %></td>
    <td><%= link_to 'Destroy', video, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Video', new_video_path %>
